# SINGLE COMMANDS
COMMAND_SINGLE_EMPTY=
COMMAND_SINGLE_CHAR=t

# COMMANDS WITH SHORT OPTIONS
COMMAND_OPTION_SHORT_SINGLE=testcommand-f
COMMAND_OPTION_SHORT_MULTI=testcommand -f-b
COMMAND_OPTION_SHORT_MULTI_PARAM_SINGLE=testcommand -f-b bar

# LONG OPTIONS
COMMAND_OPTION_LONG_SINGLE=testcommand--foo
COMMAND_OPTION_LONG_MULTI=testcommand --foo--bar
COMMAND_OPTION_LONG_MULTI_PARAM_SINGLE=testcommand --foo--bar bar

# MIXED OPTIONS
COMMAND_OPTION_MIXED=testcommand -f--bar
COMMAND_OPTION_MIXED_PARAM=testcommand -f--bar bar


