# SINGLE COMMANDS
COMMAND_SHORTEST=te
COMMAND_SINGLE_WORD=testcommand
COMMAND_SINGLE_ARGUMENT=testcommand foo
COMMAND_MULTI_ARGUMENT=testcommand foo bar

# COMMANDS WITH SHORT OPTIONS
COMMAND_OPTION_SHORT_SINGLE_SPACE=testcommand -f
COMMAND_OPTION_SHORT_SINGLE_PARAM=testcommand -f foo
COMMAND_OPTION_SHORT_MULTI_SPACE=testcommand -f -b
COMMAND_OPTION_SHORT_MULTI_SPACE_PARAM_SINGLE=testcommand -f foo -b
COMMAND_OPTION_SHORT_MULTI_SPACE_PARAM_MULTI=testcommand -f foo -b bar

# LONG OPTIONS
COMMAND_OPTION_LONG_SINGLE=testcommand --foo
COMMAND_OPTION_LONG_SINGLE_PARAM=testcommand --foo foo
COMMAND_OPTION_LONG_MULTI_SPACE=testcommand --foo --bar
COMMAND_OPTION_LONG_MULTI_SPACE_PARAM_SINGLE=testcommand --foo foo --bar
COMMAND_OPTION_LONG_MULTI_SPACE_PARAM_MULTI=testcommand --foo foo --bar bar

# MIXED OPTIONS
COMMAND_OPTION_MIXED_SPACE=testcommand -f --bar
COMMAND_OPTION_MIXED_SPACE_PARAM=testcommand -f foo --bar bar


